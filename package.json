{
  "name": "unist-diff",
  "version": "2.0.0",
  "description": "Diff two unist trees",
  "license": "MIT",
  "keywords": [
    "unist",
    "node",
    "diff",
    "tree",
    "node",
    "patch"
  ],
  "repository": "syntax-tree/unist-diff",
  "bugs": "https://github.com/syntax-tree/unist-diff/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "dependencies": {
    "getprototypeof": "^1.0.0",
    "is-object": "^1.0.1",
    "unist-util-size": "^1.0.0",
    "x-is-array": "^0.1.0"
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.1",
    "nyc": "^11.0.0",
    "remark": "^8.0.0",
    "remark-cli": "^4.0.0",
    "remark-preset-wooorm": "^3.0.0",
    "tape": "^4.0.0",
    "xo": "^0.18.0"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify index.js --bare -s unistDiff > unist-diff.js",
    "build-mangle": "esmangle < unist-diff.js > unist-diff.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "max-params": "off"
    },
    "ignores": [
      "unist-diff.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
